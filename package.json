    "ts-loader": "^0.8.1",
    "typedoc": "^0.4.4",
    "typescript": "^1.8.0",
    "typings": "^1.0.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-stream": "^3.2.0"
  }
